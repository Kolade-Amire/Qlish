<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Qlish" parent="Theme.MaterialComponents.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/logo_fill</item>
        <item name="colorPrimaryVariant">@color/app_white</item>
        <item name="colorOnPrimary">@color/logo_fill_dark</item>
        <item name="colorPrimaryDark">@color/logo_fill_dark</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/logo_fill_dark</item>
        <item name="colorSecondaryVariant">@color/logo_fill_dark_faded</item>
        <item name="colorOnSecondary">@color/logo_fill</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <item name="bottomNavigationStyle">@style/Widget.App.BottomNavigationView</item>
    </style>
</resources>